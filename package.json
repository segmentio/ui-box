{
  "name": "ui-box",
  "version": "4.0.0-0",
  "description": "Blazing Fast React UI Primitive",
  "contributors": [
    "Jeroen Ransijn (https://twitter.com/jeroen_ransijn)",
    "Roland Warmerdam (https://roland.codes)",
    "Meichen Zhou (https://github.com/jfmaggie)",
    "Netto Farah (https://twitter.com/nettofarah)",
    "Matt Shwery (https://github.com/mshwery)"
  ],
  "keywords": [
    "react"
  ],
  "repository": "segmentio/ui-box",
  "license": "MIT",
  "module": "dist/ui-box.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "sideEffects": false,
  "scripts": {
    "test": "xo && nyc ava",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "dev": "start-storybook -p 9009",
    "build": "tsc",
    "build-storybook": "build-storybook -s .storybook/static -o .out",
    "release": "np",
    "benchmark": "echo ui-box && react-benchmark dist/tools/benchmarks/box.js",
    "size": "size-limit"
  },
  "dependencies": {
    "@emotion/hash": "^0.7.1",
    "inline-style-prefixer": "^5.0.4",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/enzyme": "^3.9.1",
    "@types/inline-style-prefixer": "^5.0.0",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "babel-loader": "^8.1.0",
    "csstype": "^2.6.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "husky": "^2.2.0",
    "react": "^16.8.4",
    "react-benchmark": "^2.1.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.8.4",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.8.4",
    "size-limit": "^1.3.1",
    "ts-loader": "^8.0.1",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "webpack": false,
      "path": "dist/ui-box.cjs.production.min.js",
      "limit": "5 KB",
      "running": false,
      "gzip": false
    }
  ]
}
